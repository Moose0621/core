{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["."],
        "exclude": ["./data/index.mjs"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|coverage|docs|dist)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/tsd-jsdoc/dist/plugin"
    ],
    "opts": {
        "destination": "./",
        "template": "node_modules/tsd-jsdoc/dist",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": false
        }
    }
}