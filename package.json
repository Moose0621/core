  "description": "Common components for Paychex Flex.",
  "main": "dist/index.js",
  "author": "Dan Barnes",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
